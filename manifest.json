{
  "name": "Salesforce Scraper",
  "version": "1.3.1",
  "manifest_version": 4,
  "description": "Scrapes salesforce using jQuery, sending information to the OA sales portal",
  "update_url": "https://s3.amazonaws.com/orderahead-production/chrome_extensions/chrome-salesforce-scraper-updates.xml",
  "permissions": ["tabs", "http://*/", "https://*/"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "name": "OA Salesforce Scraper",
    "icons": ["logo.png"],
    "default_icon": "logo.png"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}